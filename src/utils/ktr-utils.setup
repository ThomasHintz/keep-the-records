(compile -s -O3 -d2 ktr-utils.scm -j ktr-utils)
(compile -s ktr-utils.import.scm -O3 -d2)
(compile -c -O3 -d2 ktr-utils.scm -unit ktr-utils -j ktr-utils)

(install-extension
  'ktr-utils
  ; Files to install for your extension:
  '("ktr-utils.o" "ktr-utils.so" "ktr-utils.import.so")
  ; Assoc list with properties for your extension:
  '((version 1)))
